<core:FragmentDefinition xmlns:tnt="sap.tnt" xmlns:cards="sap.f.cards" 
xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:m="sap.m"     
xmlns="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:u="sap.ui.unified"
    xmlns:c="sap.ui.core">   
<Table id="tblPowerFactor"
                ariaLabelledBy="title"
                rows="{NonEngModel>/}"
                enableColumnFreeze="true">
                <extension>
                    <m:OverflowToolbar style="Clear">
                        <m:Title id="title" text="Line Items"/>
                    </m:OverflowToolbar>
                </extension>
                <columns>
                    <Column
                        width="11rem">
                        <m:Label text="BP Number" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>CustomerNo}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="BP Name" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>CustomerName}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Address" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>Address}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Contract Account" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>ContractAccount}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Equipment" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>Equipment}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Device Location" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>DeviceLocation}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Installation" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>Installation}"/>
                        </template>
                    </Column>
                    <!-- <Column
                        width="11rem">
                        <m:Label text="Billing Period" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>BillingPeriod}"/>
                        </template>
                    </Column> -->
                    <Column
                        width="11rem">
                        <m:Label text="Start Date" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>StartDate}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="End Date" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>EndDate}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Power Factor(Current Month)" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>CurrentMonthPwrFactor}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Power Factor(Last Month)" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>LastMonthPwrFactor}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Power Factor(Last Year Same Month)" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>LastYearPwrFactor}"/>
                        </template>
                    </Column>
                    <Column
                        width="11rem">
                        <m:Label text="Power Factor Difference" textAlign="Center" width="100%"/>
                        <template>
                            <m:Text text="{NonEngModel>PwrFactorDiff}"/>
                        </template>
                    </Column>
                </columns>
        </Table>  
</core:FragmentDefinition>